{
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Produtos no Armazém",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "allowAdd": false,
                  "allowDelete": false,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "filterProperty": "Product.category",
                      "property": "id_product",
                      "sortProperty": "Product.category",
                      "template": "${data.Product?.category}",
                      "title": "ID Produto",
                      "type": "string"
                    },
                    {
                      "property": "name",
                      "title": "Nome",
                      "type": "string"
                    },
                    {
                      "property": "quantity",
                      "title": "Quantidade Conjunto",
                      "type": "integer"
                    },
                    {
                      "property": "total_quantity",
                      "title": "Quantidade Total",
                      "type": "integer"
                    },
                    {
                      "format": "double",
                      "property": "purchase_price",
                      "title": "Preço Compra",
                      "type": "number"
                    },
                    {
                      "property": "minimum_quantity",
                      "title": "Quantidade Mínima",
                      "type": "integer"
                    }
                  ],
                  "confirmDelete": false,
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "count": "${getProductsInWarehousesCount}",
                  "data": "${getProductsInWarehousesResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete ProductsInWarehouse",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "sql_project_final.deleteProductsInWarehouse",
                        "parameters": [
                          {
                            "name": "id_warehouse",
                            "value": "${event.id_warehouse}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "ProductsInWarehouse deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load ProductsInWarehouses",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "sql_project_final.getProductsInWarehouses",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Warehouse,Product"
                          }
                        ],
                        "then": [
                          {
                            "name": "getProductsInWarehousesResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getProductsInWarehousesCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": []
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main",
  "name": "Produtos no Armazém"
}