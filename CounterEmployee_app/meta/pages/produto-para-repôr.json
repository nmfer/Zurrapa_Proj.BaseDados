                  },
                  "type": "card"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowDelete": false,
                  "allowFiltering": true,
                  "allowPaging": true,
                  "allowSorting": true,
                  "columns": [
                    {
                      "filterProperty": "Product.category",
                      "property": "id_product",
                      "sortProperty": "Product.category",
                      "template": "${data.Product?.category}",
                      "title": "ID Produto",
                      "type": "string"
                    },
                    {
                      "property": "name",
                      "title": "Nome",
                      "type": "string"
                    },
                    {
                      "property": "quantity",
                      "title": "Quantidade Existente",
                      "type": "integer"
                    },
                    {
                      "filterProperty": "Bar.address",
                      "property": "id_bar",
                      "sortProperty": "Bar.address",
                      "template": "${data.Bar?.address}",
                      "title": "ID do Bar",
                      "type": "string"
                    }
                  ],
                  "confirmDelete": false,
                  "count": "${getProductsInBarsCount}",
                  "data": "${getProductsInBarsResult}",
                  "events": {
                    "loadData": [
                      {
                        "name": "sql_project_final.getProductsInBars",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Product,Bar"
                          }
                        ],
                        "then": [
                          {
                            "name": "getProductsInBarsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getProductsInBarsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "grid0",
                  "pageSize": 10,
                  "type": "grid",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteCancelText": "Cancel",
                  "emptyText": "No records to display.",
                  "addText": "Add",
                  "clearFilterText": "Clear",
                  "applyFilterText": "Apply",
                  "equalsText": "Equals",
                  "notEqualsText": "Not equals",
                  "lessThanText": "Less than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "greaterThanText": "Greater than",
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "endsWithText": "Ends with",
                  "containsText": "Contains",
                  "startsWithText": "Starts with"
                }
              ],
              "name": "col0",
              "type": "col"
            }
          ],
          "name": "row0",
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      },
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
