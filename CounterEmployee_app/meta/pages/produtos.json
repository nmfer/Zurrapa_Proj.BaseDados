{
  "components": [
    {
      "components": [
        {
          "name": "Produtos no Bar",
          "size": "H1",
          "text": "Produtos ",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "allowAdd": false,
                  "allowDelete": false,
                  "allowFiltering": true,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "filterProperty": "Product.category",
                      "property": "id_product",
                      "sortProperty": "Product.category",
                      "template": "${data.Product?.category}",
                      "title": "ID",
                      "type": "string"
                    },
                    {
                      "property": "name",
                      "title": "Nome",
                      "type": "string"
                    },
                    {
                      "format": "double",
                      "property": "sale_price",
                      "title": "Preço Venda",
                      "type": "number"
                    },
                    {
                      "property": "quantity",
                      "title": "Quantidade Existente",
                      "type": "integer"
                    },
                    {
                      "property": "minimum_quantity",
                      "title": "Quantidade Mínima por Produto",
                      "type": "integer"
                    }
                  ],
                  "confirmDelete": false,
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "count": "${getProductsInBarsCount}",
                  "data": "${getProductsInBarsResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-products-in-bar",
                        "type": "popup"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete ProductsInBar",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "sql_project_final.deleteProductsInBar",
                        "parameters": [
                          {
                            "name": "id_product",
                            "value": "${event.id_product}"
                          },
                          {
                            "name": "name",
                            "value": "${event.name}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "ProductsInBar deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load ProductsInBars",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "sql_project_final.getProductsInBars",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "Product"
                          }
                        ],
                        "then": [
                          {
                            "name": "getProductsInBarsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getProductsInBarsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "id_product",
                            "value": "${event.id_product}"
                          },
                          {
                            "name": "name",
                            "value": "${event.name}"
                          }
                        ],
                        "path": "edit-products-in-bar",
                        "type": "popup"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "Main_Menu",
  "name": "Produtos"
}